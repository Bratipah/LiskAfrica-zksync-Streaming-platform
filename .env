# zkTune Environment Variables
# Copy this to your .env file and fill in your values

# REQUIRED: Your wallet private key (without 0x prefix)
WALLET_PRIVATE_KEY=your_private_key_here

# Network configuration
HARDHAT_NETWORK=lisk-testnet

# Optional: RPC URLs (default values are provided in hardhat config)
LISK_TESTNET_RPC=https://rpc.sepolia-api.lisk.com
LISK_MAINNET_RPC=https://rpc.api.lisk.com

# Optional: For gas reporting
REPORT_GAS=false